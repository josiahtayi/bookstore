<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="theReadingRoom.DashboardController">
    <tabs>
        <!-- Dashboard Tab -->
        <Tab closable="false" text="Dashboard">
            <content>
                <fx:include source="Dashboard.fxml" />
            </content>
        </Tab>

        <!-- Account Management Tab -->
        <Tab closable="false" text="Account Management">
            <content>
                <fx:include source="EditProfile.fxml" />
            </content>
        </Tab>

        <!-- Order Management Tab -->
        <Tab closable="false" text="Order Management">
            <content>
                <fx:include source="OrderManagementTab.fxml" />
            </content>
        </Tab>
    </tabs>
</TabPane>
